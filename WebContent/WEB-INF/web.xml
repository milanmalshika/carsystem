<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		 xmlns:xsi="http:www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
		 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
		 version="3.1">
		 
		 <servlet>
		 	<servlet-name>login</servlet-name>
		 	<servlet-class>com.user.LoginServlet</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 	<servlet-name>login</servlet-name>
		 	<url-pattern>/log</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		 	<servlet-name>userinsert</servlet-name>
		 	<servlet-class>com.user.UserInsert</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 	<servlet-name>userinsert</servlet-name>
		 	<url-pattern>/insert</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		 	<servlet-name>carinsert</servlet-name>
		 	<servlet-class>com.user.CarInsert</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 	<servlet-name>carinsert</servlet-name>
		 	<url-pattern>/insertcar</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		 	<servlet-name>viewcars</servlet-name>
		 	<servlet-class>com.user.CarView</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 	<servlet-name>viewcars</servlet-name>
		 	<url-pattern>/viewcars</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		 	<servlet-name>editcar</servlet-name>
		 	<servlet-class>com.user.CarEdit</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 	<servlet-name>editcar</servlet-name>
		 	<url-pattern>/editcars</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		 	<servlet-name>updatecar</servlet-name>
		 	<servlet-class>com.user.UpdateCarServlet</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 	<servlet-name>updatecar</servlet-name>
		 	<url-pattern>/updatecar</url-pattern>
		 </servlet-mapping>
</web-app>

